{
	"name": "<%= _.slugify(appname) %>",
	"version": "0.0.0",
	"dependencies": {},
	"devDependencies": {
		"grunt": "~0.4.1",
		"grunt-contrib-copy": "*",
		"grunt-contrib-concat": "*",
		"grunt-contrib-coffee": "*",
		"grunt-contrib-uglify": "*",<% if (preprocessorSelected === 'sass') { %>
		"grunt-contrib-compass": "*",<% } else if (preprocessorSelected === 'less') { %>
		"grunt-contrib-less": "*",<% } %>
		"grunt-contrib-jshint": "*",
		"grunt-contrib-cssmin": "*",
		"grunt-contrib-connect": "*",
		"grunt-contrib-clean": "*",
		"grunt-contrib-htmlmin": "*",
		"grunt-bower-requirejs": "*",
		"grunt-contrib-requirejs": "*",
		"grunt-contrib-imagemin": "*",
		"grunt-contrib-watch": "*",<% if (testFramework === 'jasmine') { %>
    	"grunt-contrib-jasmine": "*",
    	"grunt-template-jasmine-requirejs": "*",<% } %>
		"grunt-rev": "*",
		"grunt-usemin": "*",<% if (testFramework === 'mocha') {  %>
    	"grunt-mocha": "*",<% } %>
		"grunt-open": "*",<% if (testFramework === 'qunit') {  %>
    	"grunt-contrib-qunit": "*",<% } %>
		"grunt-svgmin": "*",
		"grunt-concurrent": "*",
		"matchdep": "*",
		"connect-livereload": "*",
		"time-grunt": "*",
		"gateway": "*",
		"grunt-php2html": "*",
		"load-grunt-tasks": "*",
		"grunt-bower-requirejs": "*"
	},
	"engines": {
    	"node": ">=0.8.0"
	}
}
